<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		@* <title>@ViewData["Title"] - noa</title> *@

		<!-- FAVICON -->
		<link rel="shortcut icon" type="image/x-icon" href="~/assets/images/brand/favicon.ico" />

		<!-- TITLE -->
		<title>Digiproj - Bank Indonesia</title>
		
		<!-- BOOTSTRAP CSS -->
		<link id="style" href="~/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" asp-append-version="true" />

		<!-- STYLE CSS -->
		<link href="~/assets/css/login.css" rel="stylesheet" asp-append-version="true" />
		<link href="~/assets/css/parrallax.css" rel="stylesheet" />
		<link href="~/assets/css/skin-modes.css" rel="stylesheet" asp-append-version="true"/>

		<!--- FONT-ICONS CSS -->
		<link href="~/assets/css/icons.css" rel="stylesheet" asp-append-version="true"/>
		<link href='https://fonts.googleapis.com/css?family=Lato:300,400,700' rel='stylesheet' type='text/css'>



	</head>
<div id='stars'></div>
<div id='stars2'></div>
<div id='stars3'></div>
	<body class="ltr login-img">

		<!-- GLOABAL LOADER -->
		<div id="global-loader">
			<img src="~/assets/images/loader.svg" class="loader-img" alt="Loader">
		</div>
		<!-- /GLOABAL LOADER -->

	<!-- PAGE -->
	<div class="page">
		<div>
			@RenderBody()
		</div>
	</div>

			<!-- JQUERY JS -->
			<script src="~/assets/js/jquery.min.js"></script>

			<!-- BOOTSTRAP JS -->
			<script src="~/assets/plugins/bootstrap/js/popper.min.js"></script>
			<script src="~/assets/plugins/bootstrap/js/bootstrap.min.js"></script>

			<!-- Perfect SCROLLBAR JS-->
			<script src="~/assets/plugins/p-scroll/perfect-scrollbar.js"></script>


			@* @RenderSection("Scripts",false/*required*/) *@
			<script>var hostUrl = "/";</script>
			@await Component.InvokeAsync("Notyf")
			@await RenderSectionAsync("Scripts", required: false)

			<!-- COLOR THEME JS -->
			<script src="~/assets/js/themeColors.js"></script>

			<!-- CUSTOM JS -->
			<script src="~/assets/js/custom.js"></script>

			
	</body>
</html>
